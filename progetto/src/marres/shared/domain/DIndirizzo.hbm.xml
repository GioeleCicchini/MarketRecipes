<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="marres.shared.domain.DIndirizzo" table="Indirizzi" >
		<id name="id" >
			<column name="Indirizzo_Id" />
			<generator class="identity" />
		</id>
		
		<property name="indirizzo">
			<column name="Indirizzo"></column>
		</property>
		
		<property name="numCiv">
			<column name="NumeroCivico"></column>
		</property>
		
		<property name="cap">
			<column name="Cap"></column>
		</property>
	
	
	
	</class>
</hibernate-mapping>